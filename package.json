{
  "nativescript": {
    "id": "com.pixeldog.pocketvault",
    "tns-ios": {
      "version": "6.1.0"
    },
    "tns-android": {
      "version": "6.3.1"
    }
  },
  "scripts": {
    "bundle": "tns build android --release --bundle --clean --key-store-path ../!keystore/pocketvault.keystore --key-store-password process.env.key-store-password --key-store-alias pocketvault --key-store-alias-password process.env.key-store-alias-password --aab --copy-to d:/Project/pocketvault/pocketvault-release.aab",
    "build": "tns build android --release --clean --key-store-path ../!keystore/pocketvault.keystore --key-store-password process.env.key-store-password --key-store-alias pocketvault --key-store-alias-password process.env.key-store-alias-password --copy-to d:/Project/pocketvault/pocketvault-release.apk",
    "showkey": "keytool -exportcert -list -v -alias pocketvault -keystore ../!keystore/pocketvault.keystore"
  },
  "description": "A simple mobile application to store small bits of personal information safely",
  "license": "SEE LICENSE IN license.txt",
  "repository": "https://github.com/tomcatmwi/pocketvault",
